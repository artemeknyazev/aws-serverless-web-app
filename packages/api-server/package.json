  "main": "src/index.ts",
  "scripts": {
    "build": "npx webpack",
    "build:watch": "npm run build -- --watch",
    "check": "npx tsc --project tsconfig.json --noEmit",
    "check:watch": "npm run check -- --watch",
    "prettier": "npx prettier 'src/**/*' tsconfig.json tslint.json webpack.config.js .prettierrc.json",
