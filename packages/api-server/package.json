    "pretty:check": "npm run prettier -- -c",
    "lint": "npx tslint -c tslint.json 'src/**/*'",
    "clean": "npx rimraf ./build",
    "ci": "npx npm-run-all check lint clean build",
    "pre-commit": "npx npm-run-all pretty:check lint check"
